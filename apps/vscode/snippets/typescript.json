{
  "Console Log": {
    "prefix": "clg",
    "body": "console.log('${1:label}:', ${1:label});",
    "description": "Console log with label"
  },
  "Console Log String": {
    "prefix": "cls",
    "body": "console.log('$0');",
    "description": "Console log string"
  },
  "Console Error": {
    "prefix": "cle",
    "body": "console.error('${1:label}:', ${1:label});",
    "description": "Console error with label"
  },
  "Console Warn": {
    "prefix": "clw",
    "body": "console.warn('${1:label}:', ${1:label});",
    "description": "Console warn with label"
  },
  "Console Table": {
    "prefix": "clt",
    "body": "console.table(${1:data});",
    "description": "Console table"
  },
  "Console Time": {
    "prefix": "cltime",
    "body": ["console.time('${1:label}');", "$0", "console.timeEnd('${1:label}');"],
    "description": "Console time measurement"
  },
  "Import": {
    "prefix": "imp",
    "body": "import { $2 } from '${1:module}';",
    "description": "Named import"
  },
  "Import Default": {
    "prefix": "impd",
    "body": "import ${2:name} from '${1:module}';",
    "description": "Default import"
  },
  "Import All": {
    "prefix": "impa",
    "body": "import * as ${2:name} from '${1:module}';",
    "description": "Import all as namespace"
  },
  "Export": {
    "prefix": "exp",
    "body": "export { $0 };",
    "description": "Named export"
  },
  "Export Default": {
    "prefix": "expd",
    "body": "export default $0;",
    "description": "Default export"
  },
  "Arrow Function": {
    "prefix": "af",
    "body": "const ${1:name} = (${2:params}) => {\n\t$0\n};",
    "description": "Arrow function"
  },
  "Arrow Function Inline": {
    "prefix": "afi",
    "body": "const ${1:name} = (${2:params}) => $0;",
    "description": "Inline arrow function"
  },
  "Async Arrow Function": {
    "prefix": "aaf",
    "body": "const ${1:name} = async (${2:params}) => {\n\t$0\n};",
    "description": "Async arrow function"
  },
  "Function": {
    "prefix": "fn",
    "body": ["function ${1:name}(${2:params}): ${3:void} {", "\t$0", "}"],
    "description": "Function declaration"
  },
  "Async Function": {
    "prefix": "afn",
    "body": ["async function ${1:name}(${2:params}): Promise<${3:void}> {", "\t$0", "}"],
    "description": "Async function declaration"
  },
  "Interface": {
    "prefix": "int",
    "body": ["interface ${1:Name} {", "\t${2:property}: ${3:type};", "\t$0", "}"],
    "description": "Interface"
  },
  "Type": {
    "prefix": "type",
    "body": "type ${1:Name} = ${2:type};",
    "description": "Type alias"
  },
  "Type Object": {
    "prefix": "typeo",
    "body": ["type ${1:Name} = {", "\t${2:property}: ${3:type};", "\t$0", "};"],
    "description": "Object type alias"
  },
  "Enum": {
    "prefix": "enum",
    "body": ["enum ${1:Name} {", "\t${2:Value} = '${2:Value}',", "\t$0", "}"],
    "description": "Enum"
  },
  "Try Catch": {
    "prefix": "try",
    "body": ["try {", "\t$1", "} catch (error) {", "\t${2:console.error(error);}", "}"],
    "description": "Try-catch block"
  },
  "Try Catch Finally": {
    "prefix": "tryf",
    "body": ["try {", "\t$1", "} catch (error) {", "\t${2:console.error(error);}", "} finally {", "\t$0", "}"],
    "description": "Try-catch-finally block"
  },
  "Async IIFE": {
    "prefix": "iife",
    "body": ["(async () => {", "\t$0", "})();"],
    "description": "Async IIFE"
  },
  "Promise": {
    "prefix": "prom",
    "body": ["new Promise<${1:void}>((resolve, reject) => {", "\t$0", "})"],
    "description": "New Promise"
  },
  "Await": {
    "prefix": "aw",
    "body": "const ${1:result} = await ${2:promise};",
    "description": "Await expression"
  },
  "For Of": {
    "prefix": "forof",
    "body": ["for (const ${1:item} of ${2:items}) {", "\t$0", "}"],
    "description": "For-of loop"
  },
  "For In": {
    "prefix": "forin",
    "body": ["for (const ${1:key} in ${2:object}) {", "\t$0", "}"],
    "description": "For-in loop"
  },
  "ForEach": {
    "prefix": "foreach",
    "body": "${1:array}.forEach((${2:item}) => {\n\t$0\n});",
    "description": "Array forEach"
  },
  "Map": {
    "prefix": "map",
    "body": "${1:array}.map((${2:item}) => $0)",
    "description": "Array map"
  },
  "Filter": {
    "prefix": "filter",
    "body": "${1:array}.filter((${2:item}) => $0)",
    "description": "Array filter"
  },
  "Reduce": {
    "prefix": "reduce",
    "body": "${1:array}.reduce((${2:acc}, ${3:item}) => {\n\t$0\n\treturn ${2:acc};\n}, ${4:initialValue})",
    "description": "Array reduce"
  },
  "Find": {
    "prefix": "find",
    "body": "${1:array}.find((${2:item}) => $0)",
    "description": "Array find"
  },
  "Destructure Object": {
    "prefix": "dob",
    "body": "const { ${2:prop} } = ${1:object};",
    "description": "Object destructuring"
  },
  "Destructure Array": {
    "prefix": "dar",
    "body": "const [${2:first}] = ${1:array};",
    "description": "Array destructuring"
  },
  "Spread Object": {
    "prefix": "spread",
    "body": "{ ...${1:object}, ${2:key}: ${3:value} }",
    "description": "Spread object with override"
  },
  "Ternary": {
    "prefix": "ter",
    "body": "${1:condition} ? ${2:true} : ${3:false}",
    "description": "Ternary operator"
  },
  "Nullish Coalescing": {
    "prefix": "nc",
    "body": "${1:value} ?? ${2:default}",
    "description": "Nullish coalescing"
  },
  "Optional Chaining": {
    "prefix": "oc",
    "body": "${1:object}?.${2:property}",
    "description": "Optional chaining"
  },
  "Set Timeout": {
    "prefix": "timeout",
    "body": ["setTimeout(() => {", "\t$0", "}, ${1:1000});"],
    "description": "setTimeout"
  },
  "Set Interval": {
    "prefix": "interval",
    "body": ["const ${1:intervalId} = setInterval(() => {", "\t$0", "}, ${2:1000});"],
    "description": "setInterval"
  },
  "Describe Block": {
    "prefix": "desc",
    "body": ["describe('${1:description}', () => {", "\t$0", "});"],
    "description": "Test describe block"
  },
  "It Block": {
    "prefix": "it",
    "body": ["it('${1:should}', () => {", "\t$0", "});"],
    "description": "Test it block"
  },
  "It Async": {
    "prefix": "ita",
    "body": ["it('${1:should}', async () => {", "\t$0", "});"],
    "description": "Async test it block"
  },
  "Expect": {
    "prefix": "exp",
    "body": "expect(${1:value}).${2|toBe,toEqual,toBeTruthy,toBeFalsy,toContain,toThrow|}($3);",
    "description": "Expect assertion"
  },
  "TODO": {
    "prefix": "todo",
    "body": "// TODO: $0",
    "description": "TODO comment"
  },
  "FIXME": {
    "prefix": "fixme",
    "body": "// FIXME: $0",
    "description": "FIXME comment"
  },
  "JSDoc": {
    "prefix": "jsdoc",
    "body": ["/**", " * ${1:Description}", " * @param {${2:type}} ${3:param} - ${4:description}", " * @returns {${5:type}} ${6:description}", " */"],
    "description": "JSDoc comment"
  },
  "JSDoc Simple": {
    "prefix": "jsd",
    "body": "/** ${1:Description} */",
    "description": "Single-line JSDoc"
  }
}
